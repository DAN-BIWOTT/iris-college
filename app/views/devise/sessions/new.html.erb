            </div><!-- /.row -->  
        </div><!-- /.container -->                      
    </div><!-- /.page-title --> 

    <!-- Login -->
    <div class="flat-row pad-top90px">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="flat-login">
                      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                        <form class="flat-contact-form border-white border-large" id="contactform5">
                            <div class="field clearfix">      
                                <div class="wrap-type-input">                    
                                    <div class="input-wrap name">
                                        <p>Email address *</p>
                                        <%= f.email_field :email, autofocus: true, autocomplete: "email",required: true, tabindex:'1' %>
                                    </div>
                                    <div class="input-wrap email">
                                        <p>Password *</p>
                                        <%= f.password_field :password, autocomplete: "current-password",tabindex:'2',:id=>'email-contact' %>
                                    </div>                                    
                                </div>
                            </div>
                            <div class="submit-wrap">
                                <button type="submit">login</button>
                            </div>
                            <div class="option-login">
                                <div class="remember">
                                   <%= f.check_box :remember_me,:id=>'check3',checked: true%>
                                   <label for="check3" class="css-label">Remember me</label>
                                </div>
                                <div class="forgot">
                                    <%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
                                      <%= link_to "I forgot my password", new_password_path(resource_name) %>
                                    <% end -%>
                                </div>
                            </div>
                        </form><!-- /.comment-form -->
                        <% end %>
                    </div><!-- /.flat-login -->
                </div><!-- /.col-md-6 .col-md-offset-3 -->
            </div><!-- /.row -->
        </div><!-- /.container -->
    </div>
   
