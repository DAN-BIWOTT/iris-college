    <!-- Page title -->
    <div class="page-title parallax parallax1">
        <div class="overlay"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">                    
                    <div class="page-title-heading">
                        <h1 class="title">Login</h1>
                        <div class="breadcrumbs">
                            <ul>
                                <li><%= link_to 'Home',root_path %></li>
                            </ul>                   
                        </div>
                    </div><!-- /.page-title-captions --> 
                    
